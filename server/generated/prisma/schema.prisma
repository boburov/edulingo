generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Role {
  ADMIN
  USER
}

model User {
  id       String @id @default(uuid())
  username String
  surname  String
  email    String @unique
  role     Role   @default(USER)
}
